.page-header
  %h3 Listing reports

.table-responsive
  %table.table
    %thead
      %tr
        %th Result
        %th Created at
        %th
        %th

    %tbody
      - @reports.each do |report|
        %tr
          %td= report.result
          %td= report.created_at
          %td= link_to 'Show', report
          %td= link_to 'Destroy', report, data: { confirm: 'Are you sure?' }, method: :delete

= will_paginate @reports, renderer: BootstrapPagination::Rails